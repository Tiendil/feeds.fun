{
  "$id": "https://feeds.fun/schemas/identity.default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default Identity Schema",
  "type": "object",
  "properties": {
    "traits": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "format": "email"
        },
        "name": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": ["email"],
      "additionalProperties": false
    }
  }
}
